r_code === 0) {\n              this.$notify({\n                title: this.$t(\"table.success\"),\n                message: this.$t(\"smtp.updateSmtp\"),\n                type: \"success\",\n                duration: 2000\n              });\n            }\n          });\n        }\n      });\n    },\n    //重置\n    resetForm() {\n      this.$refs.dataForm.resetFields();\n    }\n  }\n};\n</script>\n\n\n\n// WEBPACK FOOTER //\n// src/views/Smtp.vue","import request from '@/utils/request'; //axios默认配置\n\n//邮件服务器设置\nexport function getSmtp(data){\n  return request({\n    url: '/admin/system/smtpdata',\n    method: 'get',\n    params: data\n  });\n}\n\n//邮件服务器设置\nexport function smtpSetting(data){\n  return request({\n    url: '/admin/system/smtp',\n    method: 'post',\n    data\n  });\n}\n\n\n// WEBPACK FOOTER //\n// ./src/api/system.js","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',{staticClass:\"smtp\"},[_c('h2',[_vm._v(_vm._s(_vm.generateTitle('smtp')))]),_vm._v(\" \"),_c('el-form',{ref:\"dataForm\",attrs:{\"model\":_vm.dataForm,\"labelWidth\":\"180px\",\"rules\":_vm.rules}},[_c('el-form-item',{attrs:{\"label\":_vm.$t('smtp.smtp_ip'),\"prop\":\"smtp_ip\"}},[_c('el-input',{model:{value:(_vm.dataForm.smtp_ip),callback:function ($$v) {_vm.$set(_vm.dataForm, \"smtp_ip\", $$v)},expression:\"dataForm.smtp_ip\"}})],1),_vm._v(\" \"),_c('el-form-item',{attrs:{\"label\":_vm.$t('smtp.smtp_port'),\"prop\":\"smtp_port\"}},[_c('el-input',{model:{value:(_vm.dataForm.smtp_port),callback:function ($$v) {_vm.$set(_vm.dataForm, \"smtp_port\", $$v)},expression:\"dataForm.smtp_port\"}})],1),_vm._v(\" \"),_c('el-form-item',{attrs:{\"label\":_vm.$t('smtp.is_encrypt'),\"prop\":\"is_encrypt\"}},[_c('el-radio-group',{model:{value:(_vm.dataForm.is_encrypt),callback:function ($$v) {_vm.$set(_vm.dataForm, \"is_encrypt\", $$v)},expression:\"dataForm.is_encrypt\"}},[_c('el-radio',{attrs:{\"label\":0}},[_vm._v(_vm._s(_vm.$t('smtp.no')))]),_